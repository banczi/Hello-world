function dodawanie(first, second) {
    a = first;
    b = second;
    return a + second;
}
console.log("1+2=" + dodawanie(1, 2));
console.log("a=" + a);
console.log("b=" + b);

console.log("54+22=" + dodawanie(54, 22));
console.log("a=" + a);
console.log("b=" + b);

