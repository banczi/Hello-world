//var promien = 4;
function obwodKola(promien){
    return 2 * promien * Math.PI;
}
function poleKola(promien){
    return (Math.pow(promien,2) * Math.PI) ;
}
console.log("obwod kola o promieniu wynosi " + obwodKola(4).toFixed(3) + "cm");
console.log("pole kola o promieniu wynosi " + poleKola(4).toFixed(3) + "cm^2");





